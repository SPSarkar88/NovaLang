function greetUser(userName) {
    return "Hello, " + userName + "! Welcome to NovaLang.";
}

function addNumbers(a, b) {
    return a + b;
}

function getStatus(score) {
    if (score >= 80) {
        return "Excellent";
    } else if (score >= 60) {
        return "Good";
    } else {
        return "Needs work";
    }
}

// Using functions
print(greetUser("Bob"));
print("5 + 7 =", addNumbers(5, 7));
print("Score 85:", getStatus(85));
print("Score 45:", getStatus(45));

