// Complex data structures
let inventory = [
    {
        sku: "LAPTOP001",
        product: { name: "Gaming Laptop", brand: "TechPro" },
        stock: 15,
        price: 1299.99,
        specs: { ram: "16GB", storage: "1TB SSD" }
    },
    {
        sku: "MOUSE001", 
